{
    "/**/": "模板文件，项目中必须以xxx.pom.json命名并放置在templates文件夹下，严格遵循以下格式",
    "properties" :  [
        {
            "sign" :  "delete",
            "data" : [
                {
                    "artifactId" : "dubbo-version"
                },
                {
                    "artifactId" : "spring-boot.version"
                }
            ]
        },
        {
            "sign" : "add",
            "data" :  [
                   {
                       "label" :  "spring-boot.version",
                       "value" : "2.5.3"
                   },
                   { 
                       "label" :  "spring-cloud.version",
                       "value" : "2020.0.3"
                   },
                   {   "label" :  "spring-cloud-huawei.version",
                       "value" : "1.8.0-2020.0.x"
                   }
            ]
        }
    ],
    "dependencyManagement.dependencies" :  [
        {
            "sign" :  "delete",
            "data" :  [
                {
                    "artifactId" : "dubbo-dependencies-bom"
                },
                {
                    "artifactId" : "spring-boot-dependencies"
                }
            ]
        },
        {
            "sign" :  "add",
            "data" :  [
                    {
                        "groupId" :  "org.springframework.boot",
                        "artifactId" : "spring-boot-dependencies",
                        "version" : "${spring-boot.version}",
                        "type" : "pom",
                        "scope" : "import"
                    },
                   { 
                       "groupId" :  "com.huaweicloud",
                       "artifactId" : "spring-cloud-huawei-bom",
                       "version" : "${spring-cloud-huawei.version}",
                       "type" : "pom",
                       "scope" : "import"
                   },
                   { 
                       "groupId" :  "org.springframework.cloud",
                       "artifactId" : "spring-cloud-dependencies",
                       "version" : "${spring-cloud.version}",
                       "type" : "pom",
                       "scope" : "import"
                   }
			      ]
        }
    ],
	"dependencies" : [
        {
            "sign" :  "delete",
            "data" :  [
                {
                    "artifactId" : "dubbo-spring-boot-starter"
                },
                {
                    "artifactId" : "dubbo-config-spring"
                },
                {
                    "artifactId" : "dubbo-metadata-report-zookeeper"
                },
                {
                    "artifactId" : "dubbo-configcenter-zookeeper"
                },
                {
                    "artifactId" : "dubbo-registry-zookeeper"
                }
            ]
        },
        {
            "sign" :  "add",
            "data" :  [
                   { 
                       "groupId" :  "com.huaweicloud",
                       "artifactId" : "spring-cloud-starter-huawei-service-engine",
                       "version" : ""
                   },
                   { 
                       "groupId" :  "org.springframework.cloud",
                       "artifactId" : "spring-cloud-starter-openfeign",
                       "version" : ""
                   }
            ]
        }
    ],
    "build.plugins" : [
        {
            "sign" :  "replace",
            "data" :  [
                {
                    "match" : {
                        "artifactId": "pandora-boot-maven-plugin"
                    },
                    "replacement": {
                        "groupId" :  "org.springframework.boot",
                        "artifactId" : "spring-boot-maven-plugin"
                    }
                }
            ]
        }
    ]
}